{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
     ,
     [
       "transform-remove-console",
       {
         "exclude": [
           "error",
           "warn"
         ]
       }
     ]
  ]
}
